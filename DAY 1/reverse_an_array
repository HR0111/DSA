 #include<iostream>
using namespace std;

void reverse(int arr [] , int n){

int start = 0;
int end = n-1;
        while (start<=end )
        {
            swap(arr[start],arr[end]);
            start++;
            end--;
        }
        
}

int printArray(int arr[], int n ){
        for (int i = 0; i < n; i++)
        {
            cout<<arr[i]<<" ";
        }
        cout<<endl;
}

int main(){

    int arr[6] = {1,2,4,5,75,3};
    int brr[3] = {99,55,11};

    reverse(arr,6);
    reverse(brr,3);

     printArray(arr,6);
     printArray(brr,3);

}
